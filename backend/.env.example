
# Supabase URL - Your project URL
SUPABASE_URL=http://127.0.0.1:54321

# Secret key - Get this from your Supabase project settings
SUPABASE_SERVICE_ROLE_KEY=your_service_role_secret_key_here
SUPABASE_ANON_KEY=your_anon_key_here

# JWKS endpoint for JWT token verification
SUPABASE_JWKS_URL=your_jwks_url_here
JWT_ALGORITHM=your_jwt_algo_here

# JWT Secret - From your Supabase project (used for verification)
# Should match config
SUPABASE_JWT_SECRET=your_jwt_secret_here

# Embedding API URL - gotten from where ever it is hosted or when starting the uvicorn instance
EMBEDDING_API_URL=embedding_api_url_here

# Embedding API URL - gotten from the embedding API
EMBEDDING_API_KEY=embedding_api_key_here